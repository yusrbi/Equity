package annotators;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

import data.WebDocument;
import de.unihd.dbs.heideltime.standalone.HeidelTimeStandalone;
import de.unihd.dbs.heideltime.standalone.exceptions.DocumentCreationTimeMissingException;

public class HeidelTimeWrapper {
	HeidelTimeStandalone heideltime_tagger;
	private static Logger slogger_ = LoggerFactory.getLogger(WebDocument.class);
	
	public  HeidelTimeWrapper(){		
		heideltime_tagger = new HeidelTimeStandalone();
	}
	
	public void process(String text){
		try {
			String annotated_text = heideltime_tagger.process("");
			
		} catch (DocumentCreationTimeMissingException e) {
			slogger_.error(e.getMessage());
			e.printStackTrace();
		}
	}

}
