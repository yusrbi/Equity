package graph;

import graph.Edge.TYPE;

public class PotentialEdge {
	String from;
	String to;
	Double weight;
	TYPE type;
	public PotentialEdge(String from, String to, double weight, TYPE type) {
		super();
		this.from = from;
		this.to = to;
		this.weight = weight; //*  Edge.getHyper_param()[type.getVal()];
		this.type = type;
	}
	
	public String getFrom() {
		return from;
	}
	public void setFrom(String from) {
		this.from = from;
	}
	public String getTo() {
		return to;
	}
	public void setTo(String to) {
		this.to = to;
	}
	public Double getWeight() {
		return weight;
	}
	public void setWeight(double weight) {
		this.weight = weight;
	}
	public TYPE getType() {
		return type;
	}
	public void setType(TYPE type) {
		this.type = type;
	}

}
